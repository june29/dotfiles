[include]
  path = ~/.gitconfig.secret
[core]
  editor = emacs
  pager = lv -c
  quotepath = false
[pager]
  log  = diff-highlight | lv
  show = diff-highlight | lv
  diff = diff-highlight | lv
[user]
  name = june29
  email = june29.jp@gmail.com
[color]
  ui = auto
[alias]
  st   = status
  df   = diff
  dfc  = diff --cached
  aa   = add --all
  ap   = add -p
  ci   = commit
  co   = checkout
  b    = branch
  g    = grep -n
  pl   = pull
  pr   = pull --rebase
  cp   = cherry-pick
  fp   = fetch --prune
  gsub = git-gsub
  branches = branch -a
  tags     = tag
  stashes  = stash list
  remotes  = remote -v
  unstage  = reset -q HEAD --
  discard  = checkout --
  uncommit = reset --mixed HEAD~
  amend    = commit --amend
[github]
  user = june29
[push]
  default = simple
[ghq]
  root = ~/ghq
  root = ~/.go/src
[filter "lfs"]
  clean = git-lfs clean %f
  smudge = git-lfs smudge %f
  required = true
[diff]
  compactionHeuristic = true
[gpg]
  rogram = gpg2
[commit]
  gpgsign = true
