[include]
  path = ~/.gitconfig.secret
[core]
  editor = emacs
  pager = lv -c
  quotepath = false
[pager]
  log  = diff-highlight | lv
  show = diff-highlight | lv
  diff = diff-highlight | lv
[user]
  name = june29
  email = june29.jp@gmail.com
[color]
  ui = auto
[alias]
  st   = status
  df   = diff
  dfc  = diff --cached
  ap   = add -p
  ci   = commit
  co   = checkout
  b    = branch
  g    = grep -n
  pl   = pull
  pr   = pull --rebase
  cp   = cherry-pick
  gsub = git-gsub
[github]
  user = june29
[push]
  default = simple
[fetch]
  prune = true
[ghq]
  root = ~/ghq
  root = ~/.go/src
[filter "lfs"]
  clean = git-lfs clean %f
  smudge = git-lfs smudge %f
  required = true
[diff]
  compactionHeuristic = true
[gpg]
  program = gpg2
[commit]
  gpgsign = true
